    },
    "homepage": "https://github.com/groupe-sii/compodoc#readme",
    "dependencies": {
        "@types/node": "6.0.45",
        "@types/typescript": "0.4.29",
        "colors": "1.1.2",
        "commander": "2.9.0",
        "fs-extra": "0.30.0",
        "q": "1.4.1",
        "typescript": "2.0.3"
    }
