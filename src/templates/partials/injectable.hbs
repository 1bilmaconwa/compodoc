<ol class="breadcrumb">
  <li><a href="/injectables.html">Injectables</a></li>
  <li>{{name}}</li>
</ol>

<p class="io-file">
    <b>File: </b>{{injectable.file}}
</p>

{{#if injectable.methods}}
<section>
    <h3 id="methods">Methods</h3>
    <table class="table table-sm table-hover">
        <tbody>
            {{#each injectable.methods}}
                <tr>
                    <td class="col-md-3"><code>{{name}}</code></td>
                    <td class="col-md-9">
                        {{#if args}}
                            <div><i>Signature: </i><code>{{fxsignature this}}</code></div>
                        {{/if}}
                        {{#if returnType}}
                            <div><i>Return type: </i><code>{{returnType}}</code></div>
                        {{/if}}
                        {{#if description}}
                            <div class="io-description">{{description}}</div>
                        {{/if}}
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</section>
{{/if}}

{{#if injectable.properties}}
<section>
    <h3 id="inputs">Properties</h3>
    <table class="table table-sm table-hover">
        <tbody>
            {{#each injectable.properties}}
                <tr>
                    <td class="col-md-3"><code>{{name}}</code></td>
                    <td class="col-md-9">
                        {{#if type}}
                            <div><i>Type: </i><code>{{type}}</code></div>
                        {{/if}}
                        {{#if defaultValue}}
                            <div><i>Default value: </i><code>{{defaultValue}}</code></div>
                        {{/if}}
                        {{#if description}}
                            <div class="io-description">{{description}}</div>
                        {{/if}}
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</section>
{{/if}}
